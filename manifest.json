{
    "manifest_version": 2,
    "name": "Memory",
    "version": "0.1",
    "description": "my first extension",
    "icons": {
        "16": "icons/16.png",
        "19": "./icons/19.png",
        "38": "./icons/38.png",
        "48": "./icons/48.png",
        "128": "./icons/128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "./icons/19.png",
            "38": "./icons/38.png"
        },
        "default_title": "Memory",
        "default_popup": "popup.html"
    },
    // "page_action": {
    //     "default_icon": {
    //         "19": "./icons/19.png",
    //         "38": "./icons/38.png"
    //     },
    //     "default_title": "Memory",
    //     "default_popup": "popup.html"
    // },
    "permissions": ["tabs", "contextMenus", "notifications", "webRequest", "*://*.youdao.com/"],
    "background": {
        "scripts": [
            "jquery.js",
            "socket.io.js",
            "background.js"
        ]
    }
}